    AbortRetryIgnore,
    Ok,
    OkCancel,
    RetryCancel,
    YesNo,
    YesNoCancel,
    ShutdownSystem,
    OkNoWait,
    CancelTryContinue);
  THardErrorResponse = (
    ReturnToCaller,
    NotHandled,
    Abort,
    Cancel,
    Ignore,
    No,
    Ok,
    Retry,
    Yes,
    TryAgain,
    Continue);

  // Helpers
  THardErrorResponseOptionsHelper = record helper for THardErrorResponseOptions
    function ToString: string;
  end;
